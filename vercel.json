{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.fratresgravina.vercel.app"
        }
      ],
      "destination": "https://fratresgravina.vercel.app/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!images|favicon.ico|robots.txt|sitemap.xml|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
